{
  "name": "association",
  "plural": "associations",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string",
      "required": true
    },
    "image": {
      "type": "string"
    },
    "url": {
      "type": "string",
      "required": true
    },
    "fburl": {
      "type": "string"
    },
    "twturl": {
      "type": "string"
    },
    "googleurl": {
      "type": "string"
    },
    "desc": {
      "type": "string",
      "dataType": "text"
    }
  },
  "validations": [],
  "relations": {
    "users": {
      "type": "hasMany",
      "model": "user",
      "foreignKey": "assoId",
